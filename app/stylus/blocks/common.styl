.rub
  font-family 'CustomRouble'
  font-weight 500
  font-size 15px

.href
  &-wrap
    display flex
    justify-content center
    width 100%
    margin-top 25px
  &-item
    display inline-block
    vertical-align top
    position relative
    color $black
    font-family DIN W10 Medium,sans-serif
    font-weight 600
    font-size 12px
    letter-spacing .08em
    line-height 16px
    text-transform uppercase
    transition opacity .3s ease-in-out
    &:hover
      opacity .7
    &:before, &:after
      content ""
      position absolute
      top 50%
      width calc(608px - 50% - 24px)
      height 1px
      background-color rgba(0,0,0,.2)
      pointer-events none
    &:before
      left calc(50% - 608px)
    &:after
      right calc(50% - 608px)

.title
  color $black
  font-family DIN W10 Medium,sans-serif
  font-weight 600
  font-size 32px
  letter-spacing -0.015em
  line-height 36px
  text-transform uppercase
  @media (max-width: 767px)
    font-size 24px
    line-height 30px
  &--white
    color #fff

.swiper-slide-hide
  width 0 !important
  overflow hidden

.swiper-button
  &-prev, &-next
    .arrow-ico
      width 18px
      height 34px
  &-prev
    .arrow-ico
      transform scale(-1, 1)

.swiper-buttons-area
  @media (max-width: 767px)
    display none

.swiper-button
  &-prev, &-next
    &.swiper-button-disabled
      opacity 0 !important
      cursor default !important

.hidden-block
  height 0

.btn
  display inline-flex
  align-items center
  justify-content center
  padding 12px 24px 10px
  color #000
  font-family DIN W10 Medium,sans-serif
  font-weight 600
  font-size 12px
  line-height 24px
  letter-spacing .02em
  text-transform uppercase
  background none
  border-radius 0
  border none
  outline none
  transition opacity .3s ease-in-out,background .3s ease-in-out,color .3s ease-in-out
  &:hover
    opacity .7

.button-scroll-top
  position fixed
  bottom 360px
  right 120px
  display block
  width 48px
  height 48px
  background url('data:image/png;base64,') no-repeat center
  background-size contain
  border none
  outline none
  padding 0
  opacity 0
  visibility hidden
  transition-property opacity,visibility,top
  transition-duration 1s
  cursor pointer
  z-index 5
  &.show
    opacity 1
    visibility visible
  @media (max-width: 1239px)
    display none
